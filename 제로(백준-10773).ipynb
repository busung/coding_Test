   "source": [
    "#list를 스택으로 이용하여 문제를 해결"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "num=int(input())\n",
    "my_list=[]\n",
    "for i in range(num):\n",
    "    a = int(input())\n",
    "    if a == 0:#0이면 list에서 pop\n",
    "        my_list.pop()\n",
    "    else:#아니면 추가\n",
    "        my_list.append(a)\n",
    "\n",
    "print(sum(my_list))"
   ]
  }
